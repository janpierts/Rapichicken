@model List<CatalogoModel>
@{
    ViewData["Title"] = "Listar_Catalogo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card text-center">
  <div class="card-header">
    Catalogo
  </div>
  <div class="row" align="center">   
                @foreach (var item in Model)
                {
					<div class="col-sm-6">
						<div class="card-body">
							<div class="card" style="width: 18rem;">
								<img src="data:image/jpeg;base64," class="card-img-top" alt="...">
								<div class="card-body">
									<h5 class="card-title">@item.NProducto</h5>
									<p class="card-text">@item.Descripcion</p>
									<p class="card-text">Disponibles: @item.Stock</p>
									<a asp-action="FEditar_Pedido" asp-controller="G_Pedidos" class="btn btn-primary btn-sm" asp-route-I_ID="@item.CatalogoId">Comprar </a>
								</div>
							</div>
						</div>
                    </div>
                }
  </div>
</div>

